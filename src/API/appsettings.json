{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ServiceSettings": {
    "ServiceName": "Submission"
  },
  "ConnectionStrings": {
    "RedisCache": "redisCache:6379"
  },
  "MongoDbSettings": {
    "Host": "ocalhostl",
    "Port": "27017",
    "DatabaseName": "Submission",
    "Username": "${MONGODB_USERNAME:-admin}",
    "Password": "${MONGODB_PASSWORD:-password}"
  },
  "RedisCacheSettings": {
    "Host": "redisCache",
    "Port": "6379"

  },
  "RabbitMQSettings": {
    "Host": "${RABBITMQ_HOST:-rabbitmq}",
    "Port": "5672"
  },
  "Keycloak": {
    "Authorizationurl": "${KEYCLOAK_AUTH_URL:-http://keycloak:7080/realms/Datawrangler/protocol/openid-connect/auth}"
  },
  "Authentication": {
    "MetadataAddress": "${KEYCLOAK_METADATA_ADDRESS:-http://keycloak:7080/realms/Datawrangler/.well-known/openid-configuration}",
    "ValidIssuer": "${KEYCLOAK_ISSUER:-http://keycloak:7080/realms/Datawrangler}"
  }
}