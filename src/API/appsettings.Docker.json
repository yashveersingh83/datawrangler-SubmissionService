{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ServiceSettings": {
    "ServiceName": "Submission"
  },
  "ConnectionStrings": {
    "RedisCache": "redisCache:6379"
  },
  "MongoDbSettings": {
    "Host": "ocalhostl",
    "Port": "27017",
    "DatabaseName": "Submission",
    "Username": "${MONGODB_USERNAME:-admin}",
    "Password": "${MONGODB_PASSWORD:-password}"
  },
  "RedisCacheSettings": {
    "Host": "127.0.0.1",
    "Port": "6379"

  },
  "RabbitMQSettings": {
    "Host": "rabbitMessageQueue",
    "Port": "5672"
  },

  "Keycloak": {
    "realm": "Datawrangler",
    "Authorizationurl": "http://host.docker.internal:7080/realms/Datawrangler/protocol/openid-connect/auth"
    

  },
  "Authentication": {
    "MetadataAddress": "http://host.docker.internal:7080/realms/Datawrangler/.well-known/openid-configuration",
    "ValidIssuer": "http://host.docker.internal:7080/realms/Datawrangler",
    "Audience": "account"
  }
}